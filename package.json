{
  "name": "snippets",
  "version": "1.0.0",
  "description": "Host code snippets online",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "prebuild": "rm -rf build",
    "build": "webpack --config webpack/webpack.client.config.js",
    "build:watch": "npm run build -- --watch",
    "db:migrate": "sequelize db:migrate",
    "start": "node source/index.js",
    "dev": "nodemon source/index.js",
    "test": "mocha-webpack --webpack-config webpack/webpack.test.config.js --require tests/unit/setup.js tests/unit/specs/**/*.spec.js",
    "test:watch": "npm run test -- --watch",
    "lint": "xo --reporter table",
    "lint:fix": "xo --reporter table --fix"
  },
  "author": "Arnelle Balane <arnellebalane@gmail.com> (https://arnellebalane.com/)",
  "license": "MIT",
  "devDependencies": {
    "@vue/test-utils": "1.0.0-beta.11",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.6.1",
    "commitizen": "2.9.6",
    "copy-webpack-plugin": "4.5.1",
    "css-loader": "0.28.11",
    "cz-conventional-changelog": "2.1.0",
    "ghooks": "2.0.2",
    "html-webpack-plugin": "3.1.0",
    "jsdom": "11.6.2",
    "jsdom-global": "3.0.2",
    "mocha": "5.0.5",
    "mocha-webpack": "1.1.0",
    "nodemon": "1.17.2",
    "npm-run-all": "4.1.2",
    "sequelize-cli": "4.0.0",
    "sinon": "4.4.9",
    "style-loader": "0.20.3",
    "vue-loader": "14.2.2",
    "vue-template-compiler": "2.5.16",
    "webpack": "4.4.1",
    "webpack-cli": "2.0.13",
    "webpack-merge": "4.1.2",
    "webpack-node-externals": "1.6.0",
    "workbox-webpack-plugin": "3.0.1",
    "xo": "0.20.3"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {
      "pre-push": "npm run lint"
    }
  },
  "xo": {
    "parser": "babel-eslint",
    "space": 4,
    "envs": [
      "browser",
      "mocha",
      "serviceworker"
    ],
    "rules": {
      "operator-linebreak": [
        "error",
        "before"
      ],
      "no-await-in-loop": [
        "off"
      ],
      "import/extensions": [
        "off"
      ],
      "import/no-unresolved": [
        "off"
      ],
      "import/no-unassigned-import": [
        "error",
        {
          "allow": [
            "**/*.css"
          ]
        }
      ]
    },
    "globals": [
      "workbox"
    ],
    "overrides": [
      {
        "files": "source/database/models/!(index).js",
        "rules": {
          "unicorn/filename-case": [
            "error",
            {
              "case": "pascalCase"
            }
          ]
        }
      }
    ]
  },
  "dependencies": {
    "auto-load": "3.0.0",
    "axios": "0.18.0",
    "dotenv": "5.0.1",
    "highlightjs": "9.10.0",
    "pg": "7.4.1",
    "sequelize": "4.37.5",
    "server": "1.0.18",
    "vue": "2.5.16",
    "vue-router": "3.0.1",
    "vuex": "3.0.1",
    "workbox-sw": "3.0.1"
  }
}
