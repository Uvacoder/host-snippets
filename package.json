{
  "name": "snippets",
  "version": "1.0.0",
  "description": "Host code snippets online",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf build",
    "build": "NODE_ENV=production webpack --config webpack/client.babel.js",
    "build:watch": "npm run build -- --watch",
    "db:migrate": "sequelize db:migrate",
    "start": "node source/index.js",
    "dev": "nodemon source/index.js",
    "test": "NODE_ENV=testing mocha-webpack --webpack-config webpack/test.babel.js --require tests/unit/setup.js tests/unit/specs/**/*.spec.js",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Arnelle Balane <arnellebalane@gmail.com> (https://arnellebalane.com/)",
  "license": "MIT",
  "dependencies": {
    "auto-load": "3.0.4",
    "axios": "0.18.0",
    "dotenv": "6.2.0",
    "highlightjs": "9.12.0",
    "pg": "7.8.0",
    "sequelize": "4.42.0",
    "server": "1.0.18",
    "vue": "2.5.22",
    "vue-router": "3.0.2",
    "vuex": "3.0.1",
    "workbox-sw": "3.6.3"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "@babel/register": "7.0.0",
    "@vue/test-utils": "1.0.0-beta.11",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "2.1.0",
    "html-webpack-plugin": "3.2.0",
    "jsdom": "13.1.0",
    "jsdom-global": "3.0.2",
    "mocha": "5.2.0",
    "mocha-webpack": "1.1.0",
    "nodemon": "1.18.9",
    "npm-run-all": "4.1.5",
    "sequelize-cli": "5.4.0",
    "sinon": "7.2.2",
    "style-loader": "0.23.1",
    "vue-loader": "15.5.1",
    "vue-template-compiler": "2.5.22",
    "webpack": "4.28.4",
    "webpack-cli": "3.2.1",
    "webpack-merge": "4.2.1",
    "webpack-node-externals": "1.7.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
